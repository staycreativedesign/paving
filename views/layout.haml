  %body
    .header-wrapper.header-transparent
      / .header start
      %header#header
        .header-inner
          / .container start
          .container
            / .main-nav start
            .main-nav
              / .row start
              .row.bg-grey
                .col-md-6
                  %nav.navbar.navbar-default.nav-left{:role => "navigation"}
                    / .navbar-header start
                    .navbar-header
                      .logo.pull-left
                        %a{:href => "index.html"}
                          %h1.orange.logo-margin-fix
                            Joles Asphalt Paving
                      / .logo end

                  %p.contact-info
                    %strong Phone:
                    %span.orange 888-779-0246
                    %strong
                      Free Estimates
                .col-md-6
                  %ul.nav.navbar-nav.pull-right
                    %li.orange
                      %a{:href => "about.html", :class => "orange"} About us
                    %li
                      %a{:href => "services.html"} Services
                    %li
                      %a{:href => "contact.html"} Contact
                      / .nav.navbar-nav end
                      / #search start
                      / #search end
                    / MAIN NAVIGATION END
                  / .navbar.navbar-default end
                / .col-md-12 end
              / .row end
            / .main-nav end
          / .container end
        / .header-inner end
      / .header
    / .header-wrapper.header-transparent end
    .tp-banner-container.fullwidthbanner
      #rev-slider.tp-banner
        %ul
          / SLIDE ONE
          %li{"data-masterspeed" => "700", "data-slotamount" => "5", "data-title" => "winter", "data-transition" => "fade"}
            / MAIN IMAGE
            %img{:alt => "slidebg3", "data-bgfit" => "cover", "data-bgposition" => "center center", "data-bgrepeat" => "no-repeat", :src => "img/slider/slide-01.jpg"}/
            .tp-caption.tp-big-heading.tp-big-heading-bold.tp-white.sfb.text-center{"data-easing" => "Back.easeOut", "data-endspeed" => "500", "data-speed" => "500", "data-start" => "1000", "data-x" => "110", "data-y" => "310", :style => "z-index: 3; max-width: auto; max-height: auto; white-space: nowrap;"}
              Over
              %span 35
              years of experience
            .tp-caption.tp-small-heading.sfb{"data-easing" => "Back.easeOut", "data-endspeed" => "500", "data-speed" => "500", "data-start" => "1100", "data-x" => "360", "data-y" => "370", :style => "z-index: 3; max-width: auto; max-height: auto; white-space: nowrap;"}
              Serving state wide very proudly for over 35 years
          / SLIDE ONE END
      / .tp-banner end
    / .tp-banner-container end
    / .page-content start
    .page-content
      .container
        / .row start
        .row.mb-0.negative-top-margin
          .col-md-4.col-sm-4
            .feature-box.style-1
              .feature-body{:style => "overflow: hidden;"}
                .custom-heading.pull-left{:style => "width: 20%;"}
                  %img.img-responsive{:src => "/img/truck.png"}
                .custom-heading.pull-right.text-left{:style => "width: 75%"}
                  %h4 Free Estimates
                  %p We offer affordable estimates to match your budget!
          .col-md-4.col-sm-4
            .feature-box.style-1
              .feature-body{:style => "overflow: hidden;"}
                .custom-heading.pull-left{:style => "width: 20%;"}
                  %img.img-responsive{:src => "/img/truck.png"}
                .custom-heading.pull-right.text-left{:style => "width: 75%"}
                  %h4 Locally Owned
                  %p We are proud to be locally owned and operated for 35 years.

          .col-md-4.col-sm-4
            .feature-box.style-1
              .feature-body{:style => "overflow: hidden;"}
                .custom-heading.pull-left{:style => "width: 20%;"}
                  %img.img-responsive{:src => "/img/truck.png"}
                .custom-heading.pull-right.text-left{:style => "width: 75%"}
                  %h4 Experience
                  %p We have over 35 years of experience getting the job done!
                / .custom-heading end
              / .feature-body end
            / .feature-box-end
          / .col-md-4 end
    .page-content.quote.style-1.custom-background.bkg-darkgrey.dark.mb-70
    .page-content
      .container
        .row
          .col-md-4.col-sm-4
            .feature-box.style-2
              .feature-media
                %img.img-responsive{:alt => "Builder Construction HTML Template", :src => "img/pics/img-01.jpg"}/
              / .feature-media end
              .feature-body
                .custom-heading.style-1
                  %h4 About us
                / .custom-heading end
                %p
                  We are builder construction company from Uk with over 20 years
                  of experience in house remodeling, green building and renovations.
              / .feature-body end
            / .feature-box-end
          / .col-md-4 end
          .col-md-4.col-sm-4
            .feature-box.style-2
              .feature-media
                %img.img-responsive{:alt => "Builder Construction HTML Template", :src => "img/pics/img-02.jpg"}/
              .feature-body
                .custom-heading.style-1
                  %h4 Our services
                / .custom-heading end
                %p
                  Professional construction services from house renovations and remodeling,
                  green building and small scope work, like painting.
              / .feature-body end
            / .feature-box-end
          / .col-md-4 end
          .col-md-4.col-sm-4
            .feature-box.style-2
              .feature-media
                %img.img-responsive{:alt => "Builder Construction HTML Template", :src => "img/pics/img-03.jpg"}/
              .feature-body
                .custom-heading.style-1
                  %h4 Project management
                / .custom-heading end
                %p
                  Our highly educated staff will make sure that your project will be finished
                  on time and specified budget.
              / .feature-body end
            / .feature-box-end
          / .col-md-4 end
        / .row end
      / .container end
    / .page-content end
    / .page-content start
    .page-content.custom-background.bkg-black.dark.mb-70
      .container-fluid
        / .row start
        .row.mb-0
          / .col-md-12 start
          .col-md-12.padding-0.mb-0
            / Gallery carousel start
            .carousel-wrapper
              .carousel-container
                #gallery-carousel.owl-carousel.owl-theme
                  .item
                    .gallery-carousel-item
                      %figure
                        %img.img-responsive{:alt => "Builder Construction HTML Template", :src => "img/pics/img-04.jpg"}/
                        .hover-mask-container
                          .hover-zoom
                            Family House Remodeling Project
    #footer-wrapper
      / #footer start
      %footer#footer
        / .container start
        .container
          / .row start
          .row
            / .col-md-3 start
            .col-md-6.col-sm-12
              %h1.orange
                Joles Asphalt Paving
            / .col-md-3 end
            / .col-md-3 start
            .col-md-3.col-sm-4
            .col-md-3.col-sm-4
              .footer-info
                %strong Phone:
                888-779-0246
            / .col-md-3 end
          / .row end
        / .container end
      / #footer end
    / #footer-wrapper end
    / #copyright-container start
    #copyright-container
      / .container start
      .container
        / .row start
        .row
          / .col-md-6 start
          .col-md-6.col-sm-6
            %p
              %strong © Builder
              2016. All rights reserved
          / .col-md-6 end
          / .col-md-6 start
          .col-md-6.col-sm-6
            %ul.breadcrumb
              %li
                %a{:href => "index.html"} Home
              %li
                %a{:href => "about.html"} About
              %li
                %a{:href => "services.html"} Services
              %li
                %a{:href => "projects.html"} Projects
              %li
                %a{:href => "news.html"} News
              %li
                %a{:href => "contact.html"} Contact
          / .col-md-6 end
        / .row end
      / .container end
      %a.scroll-up{:href => "#"}
        %span.icon-container
          %img.img-responsive{:alt => "arrow icon", :src => "img/svg/arrowup.svg"}/
    / .copyright-container end
    %script{:src => "js/jquery-2.1.4.min.js"}
    / jQuery library
    %script{:src => "bootstrap/js/bootstrap.min.js"}
    / .bootstrap script
    %script{:src => "js/jquery.scripts.min.js"}
    / modernizr, retina...
    %script{:src => "rs-plugin/js/jquery.themepunch.tools.min.js"}
    / Revolution slider script
    %script{:src => "rs-plugin/js/jquery.themepunch.revolution.min.js"}
    / Revolution slider script
    %script{:src => "js/jquery.magnific-popup.min.js"}
    / used for video lightbox
    %script{:src => "owl-carousel/owl.carousel.min.js"}
    / Carousels script
    %script{:src => "js/retina.js"}
    / retina script
    %script{:src => "js/include.js"}
    / custom js functions
    :javascript
      /* <![CDATA[ */
      jQuery(document).ready(function ($) {
          'use strict';

          jQuery('#rev-slider').revolution(
                  {
                      dottedOverlay: "none",
                      delay: 9000,
                      startwidth: 1170,
                      startheight: 700,
                      hideThumbs: 200,
                      thumbWidth: 100,
                      thumbHeight: 50,
                      thumbAmount: 3,
                      navigationType: "none",
                      navigationArrows: "solo",
                      navigationStyle: "preview5",
                      touchenabled: "on",
                      onHoverStop: "off",
                      swipe_velocity: 0.7,
                      swipe_min_touches: 1,
                      swipe_max_touches: 1,
                      drag_block_vertical: false,
                      keyboardNavigation: "on",
                      navigationHAlign: "center",
                      navigationVAlign: "bottom",
                      navigationHOffset: 0,
                      navigationVOffset: 20,
                      soloArrowLeftHalign: "left",
                      soloArrowLeftValign: "center",
                      soloArrowLeftHOffset: 20,
                      soloArrowLeftVOffset: 0,
                      soloArrowRightHalign: "right",
                      soloArrowRightValign: "center",
                      soloArrowRightHOffset: 20,
                      soloArrowRightVOffset: 0,
                      shadow: 0,
                      fullWidth: "on",
                      fullScreen: "off",
                      spinner: "spinner0",
                      stopLoop: "off",
                      stopAfterLoops: -1,
                      stopAtSlide: -1,
                      shuffle: "off",
                      forceFullWidth: "off",
                      fullScreenAlignForce: "off",
                      minFullScreenHeight: "400",
                      hideThumbsOnMobile: "off",
                      hideNavDelayOnMobile: 1500,
                      hideBulletsOnMobile: "off",
                      hideArrowsOnMobile: "off",
                      hideThumbsUnderResolution: 0,
                      hideSliderAtLimit: 0,
                      hideCaptionAtLimit: 0,
                      hideAllCaptionAtLilmit: 0,
                      startWithSlide: 0,
                      hideTimerBar: "on"
                  });

          $('.video-post').magnificPopup({
              type: 'iframe',
              iframe: {
                  markup: '<div class="mfp-iframe-scaler">' +
                          '<div class="mfp-close"></div>' +
                          '<iframe class="mfp-iframe" frameborder="0" allowfullscreen></iframe>' +
                          '</div>'
              }
          });

          // GALLERY CAROUSEL
          $('#gallery-carousel').owlCarousel({
              items: 5, //5 items above 1000px browser width
              itemsCustom: [[0, 1], [600, 2], [1000, 4], [1300, 5]],
              autoPlay: true,
              pagination: false,
              navigation: true
          });

          // TESTIMONIAL CAROUSEL START
          $('#testimonial-carousel').owlCarousel({
              items: 2, //2 items above 1000px browser width
              itemsCustom: [[0, 1], [600, 2], [1000, 2]],
              autoPlay: true,
              pagination: false,
              navigation: true
          });

      });
