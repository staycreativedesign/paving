    / template styles
    %link{:href => "css/color-default.css", :rel => "stylesheet"}/
    / default template color styles
    %link{:href => "css/retina.css", :rel => "stylesheet"}/
    / retina ready styles
    %link{:href => "css/responsive.css", :rel => "stylesheet"}/
